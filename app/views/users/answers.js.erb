if ($('.activity-content .sub-tabs-answers').length !== 1) {
  $('.activity-content').html('<%= j render partial: "answers_activity_tab", locals: {answers: @user_answers, user: @user, course: @user_course} %>')
}
$('.activity-sub-content-answers').html('<%= j render partial: "activity_content", locals: {object_name: "answer", objects: @user_answers, sort: @sort, course: @user_course} %>');
$('.sub-tabs-answers .<%= @sort %>').tab('show');
$('.parent-tabs .answers-activity-tabs').tab('show');
