if ($('.activity-content .sub-tabs-questions').length !== 1) {
  $('.activity-content').html('<%= j render partial: "questions_activity_tab", locals: {questions: @user_questions, user: @user, sort: @sort} %>')
}
$('.activity-sub-content-questions').html('<%= j render partial: "activity_content", locals: {object_name: "question", objects: @user_questions, sort: @sort} %>');
$('.sub-tabs-questions .<%= @sort %>').tab('show');
$('.parent-tabs .questions-activity-tabs').tab('show');
